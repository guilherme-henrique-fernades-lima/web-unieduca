<section id="topbar" class="topbar d-flex align-items-center">
    <div class="container d-flex justify-content-center justify-content-md-between">
      <div class="contact-info d-flex align-items-center">
        <i class="bi bi-envelope d-flex align-items-center"><a href="mailto:<%=empresa.email%>"><%=empresa?.email%></a></i>
        <i class="bi bi-phone d-flex align-items-center ms-4"><span><%=empresa?.whatsapp%></span></i>
      </div>
      <div class="social-links d-none d-md-flex align-items-center">
        <%if(empresa?.twitter){%>
          <a href="<%=empresa?.twitter%>" class="twitter"><i class="bi bi-twitter"></i></a>
        <%}%>
        <%if(empresa?.facebook){%>
          <a href="<%=empresa?.facebook%>" class="facebook"><i class="bi bi-facebook"></i></a>
        <%}%>
        <%if(empresa?.instagram){%>
          <a href="<%=empresa?.instagram%>" class="instagram"><i class="bi bi-instagram"></i></a>
        <%}%>
        <%if(empresa?.likedin){%>
          <a href="<%=empresa?.likedin%>" class="linkedin"><i class="bi bi-linkedin"></i></a>
        <%}%>
      </div>
    </div>
  </section><!-- End Top Bar -->